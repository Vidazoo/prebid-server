{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Tagoras Adapter Params",
  "description": "A schema which validates params accepted by the Tagoras adapter",
  "type": "object",
  "properties": {
    "cId": {
      "type": "string",
      "description": "The connection id.",
      "minLength": 1,
      "pattern": "^[a-zA-Z0-9_]+$"
    }
  },
  "required": [
    "cId"
  ],
  "additionalProperties": false
}